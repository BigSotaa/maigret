--- /dev/null	(date 1741428049305)
+++ b/maigret-frontend/main.py	(date 1741428049305)
@@ -0,0 +1,77 @@
+from fastapi import FastAPI, Query  # ✅ Import Query
+from fastapi.middleware.cors import CORSMiddleware
+import subprocess  # ✅ Ensure subprocess is imported for running Maigret commands
+
+# ✅ Define FastAPI app instance
+app = FastAPI()
+
+# ✅ Add CORS Middleware
+app.add_middleware(
+    CORSMiddleware,
+    allow_origins=["*"],  # Allow all domains (change for production)
+    allow_credentials=True,
+    allow_methods=["*"],
+    allow_headers=["*"],
+)
+
+# ✅ Test API endpoint
+@app.get("/")
+def read_root():
+    return {"message": "FastAPI is running!"}
+
+# ✅ Search endpoint
+@app.get("/search")
+def search_user(
+    username: str,
+    tags: str = Query("", description="Filter sites by tags"),
+    max_connections: int = Query(100, description="Max concurrent connections"),
+    all_sites: bool = Query(False, description="Use all available sites"),
+    top_sites: int = Query(500, description="Number of top-ranked sites to scan"),
+    timeout: int = Query(30, description="Request timeout in seconds"),
+    cookies_file: str = Query("", description="Path to cookies file"),
+    no_recursion: bool = Query(False, description="Disable recursive search"),
+    use_disabled_sites: bool = Query(False, description="Include disabled sites"),
+    id_type: str = Query("", description="Custom identifier types"),
+    ignore_ids: bool = Query(False, description="Ignore irrelevant usernames"),
+    output_format: str = Query("json", description="Output format (json, csv, txt, pdf, html)"),
+    output_folder: str = Query("results", description="Folder to save results"),
+    verbose: bool = Query(False, description="Enable verbose mode"),
+    debug: bool = Query(False, description="Enable debug mode"),
+    retries: int = Query(3, description="Number of retry attempts")
+):
+    command = ["python", "-m", "maigret", username]
+
+    if tags:
+        command.extend(["--tags", tags])
+    if all_sites:
+        command.append("--all-sites")
+    if top_sites:
+        command.extend(["--top-sites", str(top_sites)])
+    if timeout:
+        command.extend(["--timeout", str(timeout)])
+    if cookies_file:
+        command.extend(["--cookies-jar-file", cookies_file])
+    if no_recursion:
+        command.append("--no-recursion")
+    if use_disabled_sites:
+        command.append("--use-disabled-sites")
+    if id_type:
+        command.extend(["--id-type", id_type])
+    if ignore_ids:
+        command.append("--ignore-ids")
+    if output_format:
+        command.extend(["--" + output_format])
+    if output_folder:
+        command.extend(["-fo", output_folder])
+    if verbose:
+        command.append("--verbose")
+    if debug:
+        command.append("--debug")
+    if retries:
+        command.extend(["--retries", str(retries)])
+
+    try:
+        result = subprocess.run(command, capture_output=True, text=True)
+        return {"command": " ".join(command), "output": result.stdout}
+    except Exception as e:
+        return {"error": str(e)}
